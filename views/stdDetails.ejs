<html lang="en">
<%- include("./partials/head.ejs") %>

<body>
  <%- include("./partials/nav.ejs") %>

  <div class="Student-Details">
    <h2>Student Details</h2>
    <% if (student.length > 0) { %>
      <% student.forEach(student => { %>

        <h3 class="stdName"><%= student.stdName %></h3>
        <p class="stdAddress"><%= student.stdAddress %></p>
        <p class="stdMarks"><%= student.stdMarks %></p>
        <p class="stdPercentage"><%= student.stdPercentage %></p>

      <% }) %>
    <% } else { %>
      <p>There are no blogs to display...</p>
    <% } %>
  </div>

  <%- include("./partials/footer.ejs") %>
</body>
</html>