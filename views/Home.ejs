<html lang="en">
<%- include("./partials/head.ejs") %>

<body>
  <%- include("./partials/nav.ejs") %>

  <div class="details-content">
    <h2>WELCOME</h2>
    <% if (student.length > 0) { %>
      <% student.forEach(student => { %>
        <a class="single" href="/student/<%= student._id %>">
        <h3 class="stdName"><%= student.stdName %></h3>
        <p class="stdAddress"><%= student.stdAddress %></p>
        </a>
      <% }) %>
    <% } else { %>
        <p>There are no blogs to display...</p>
    <% } %>
  </div>

  <%- include("./partials/footer.ejs") %>
</body>
</html>